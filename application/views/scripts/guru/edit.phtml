<div class="column column-wide column-wide-parent column-last">
  <div class="widget">
    <h1 class="post-title">Edit Profile </h1>
    <div class="divider">&nbsp;</div>
    <div class="post-content">
      <form class="login-form" action="#" method="post">
      <div class="widget" style="display:inline-block;">
        <div class="step-content">
          <fieldset>
            <div class="input-form">
                <label for="jenis_sek">Jenis Sekolah <em>*</em></label>
                <select name="jenis_sek" id="jenis_sek" class="input-text">
                  <option value="1">Reguler</option>
                  <option value="3">Paket</option>
                </select>
            </div>
            <div class="input-form">
                <label for="jenis_peg">Kelompok Pegawai <em>*</em></label>
                <select name="jenis_peg" class="input-text">
                  <option value="G">Tenaga Pendidik</option>
                  <option value="P">Tenaga Kependidikan</option>
                </select>
            </div>
            <div class="input-form">
              <label for="keyword">Kata Kunci</label>
              <div class="keyword-content" style="width:100%;display:inline-block;">
                <input type="text" name="keyword" value="" class="input-text" style="width:45%; margin-right:10px;">
                <select name="berdasarkan" class="input-text" style="width:40%;">
                  <option value="">-- Pilih Kriteria --</option>
                  <option value="nama">Nama</option>
                  <option value="nip">NIP</option>
                  <option value="nik">NIK</option>
                  <option value="nip">NRK</option>
                  <option value="nip">NIGB</option>
                  <option value="nuptk">NUPTK</option>
                </select>
              </div>
            </div>
            <div class="input-form">
                <label for="kec">Kecamatan <em>*</em></label>
                <select name="kec" class="input-text">
                  <option value="">-- Pilih Kecamatan --</option>
                  <option value="6408">Cakung</option>
                  <option value="6403">Cipayung</option>
                  <option value="6402">Ciracas</option>
                  <option value="6407">Duren Sawit</option>
                  <option value="6406">Jatinegara</option>
                  <option value="6405">Kramat Jati</option>
                  <option value="6404">Makasar</option>
                  <option value="6410">Matraman</option>
                  <option value="6401">Pasar Rebo</option>
                  <option value="6409">Pulo Gadung</option>
                </select>
            </div>
            <div class="input-form">
                <label for="tingkat_sekolah">Tingkat Sekolah <em>*</em></label>
                <select name="tingkat_sekolah" class="input-text" id="tingkat_sekolah">
                  <option value="">-- Pilih Tingkat Sekolah --</option>
                  <option value="">TK</option>
                  <option value="">SD</option>
                  <option value="">SMP</option>
                  <option value="">SMA</option>
                  <option value="">SMK</option>
                </select>
            </div>
            <div class="input-form">
                <label for="nama_sekolah">Nama Sekolah <em>*</em></label>
                <select name="nama_sekolah" class="input-text" id="nama_sekolah">
                  <option value="">-- Pilih Tingkat Sekolah Dahulu--</option>
                </select>
            </div>
            <div class="input-form">
              <div class="form-wide">
                <label for="umur">Umur <em>*</em></label>
                <input type="text" name="umur" id="umur" value="" class="input-text">
              </div>
              <div class="form-wide">
                <label for="masa_kerja">Masa Kerja <em>*</em></label>
                <input type="text" name="masa_kerja" id="masa_kerja" value="" class="input-text">
              </div>
            </div>
            <div class="input-form">
                <label for="jkelamin">Jenis Kelamin <em>*</em></label>
                <select name="jkelamin" class="input-text" id="jkelamin">
                  <option value="">-- Pilih --</option>
                  <option value="L">Laki-laki</option>
                  <option value="P">Perempuan</option>
                 </select>
            </div>
            <div class="input-form">
                <label for="mapel">Mata Pelajaran <em>*</em></label>
                <select name="mapel" class="input-text" id="mapel">
                  <option value="">-- Pilih --</option>
                 </select>
            </div>
            <div class="input-form">
                <label for="status">Status Sekolah <em>*</em></label>
                <select name="status" class="input-text" id="status">
                  <option value="">-- Pilih --</option>
                  <option value="1">Negeri</option>
                  <option value="2">Swasta</option>
                </select>
            </div>
            <div class="input-form">
                <label for="stat_peg">Status Kepegawaian <em>*</em></label>
                <select name="stat_peg" class="input-text" id="stat_peg">
                  <option value="">-- Pilih --</option>
                  <option value="G01">PNS</option>
                  <option value="G00">CPNS</option>
                  <option value="G04">Honor K-1 / PTT</option>
                  <option value="G13">Honor K-2</option>
                  <option value="G03">Honor Murni Negeri 2005+</option>
                  <option value="G06">PHL (K-2)</option>
                  <option value="G07">PHL (Honor Murni)</option>
                  <option value="G05">Guru Bantu</option>
                  <option value="G11">Guru DPK</option>
                  <option value="G12">Guru Depag</option>
                  <option value="G02">Guru Tetap Yayasan</option>
                  <option value="G14">Guru Tidak Tetap Yayasan</option>
                </select>
            </div>
            <div class="input-form">
              <div class="form-wide">
                <label for="gol_ruang">Golongan <em>*</em></label>
                <select name="gol_ruang" class="input-text" id="gol_ruang">
                  <option value="">-- Pilih --</option>
                  <option value="01">Gol I</option>
                  <option value="02">Gol II</option>
                  <option value="03">Gol III</option>
                  <option value="04">Gol IV</option>
                </select>
              </div>
              <div class="form-wide">
                <label for="ruang">Ruang <em>*</em></label>
                <select name="ruang" class="input-text" id="ruang">
                  <option value="">-- Pilih --</option>
                  <option value="a">a</option>
                  <option value="b">b</option>
                  <option value="c">c</option>
                  <option value="d">d</option>
                  <option value="e">e</option>
                </select>
              </div>
            </div>
            <div class="input-form">
                <label for="jabatan">Jabatan <em>*</em></label>
                <select name="jabatan" class="input-text" id="jabatan">
                  <option value="">-- Pilih Jabatan --</option>
                </select>
            </div>
            <div class="input-form">
              <div class="form-wide">
                <label for="sertifikasi">Sertifikasi <em>*</em></label>
                <select name="sertifikasi" class="input-text" id="sertifikasi">
                  <option value="">-- Pilih --</option>
                  <option value="1">Ya</option>
                  <option value="0">Tidak</option>
                </select>
             </div>
             <div class="form-wide">
               <label for="thn_sertifikasi">Tahun <em>*</em></label>
               <input type="text" name="thn_sertifikasi" id="thn_sertifikasi" value="" class="input-text">
             </div>
            </div>
            <div class="input-form">
              <div class="form-wide">
                <label for="pendidikan">Ijazah Terakhir <em>*</em></label>
                <select name="pendidikan" id="pendidikan" class="input-text">
                  <option value="">-- Pilih --</option>
                  <option value="S13">SLTA</option>
                  <option value="S21">PGSLP/D-1</option>
                  <option value="S22">PGSLA/D-2</option>
                  <option value="S23">Sarmud/D-3</option>
                  <option value="S31">Sarjana/S-1</option>
                  <option value="S32">Pasca Sarjana/S-2</option>
                  <option value="S33">Doktor/S-3</option>
                </select>
              </div>
              <div class="form-wide">
                <label for="thn_pendidikan">Tahun <em>*</em></label>
                <input type="text" name="thn_pendidikan" id="thn_pendidikan" value="" class="input-text">
              </div>
            </div>
            <div class="input-form">
                <label for="domisili">Domisili Sekarang <em>*</em></label>
                <select name="domisili" id="domisili" class="input-text">
                  <option value="">-- Pilih --</option>
                  <option value="0">DKI Jakarta</option>
                  <option value="1">Luar DKI Jakarta</option>
                </select>
            </div>
          </fieldset>
        </div>
        <div class="step-content">
              <fieldset>
                <div class="input-form">
                    <label for="nip">NIP <em>*</em></label>
                    <input type="text" name="nip" id="nip" value="" class="input-text">
                </div>
                <div class="input-form">
                    <label for="nama">Nama Lengkap <em>*</em></label>
                    <input type="text" name="nama" id="nama" value="" class="input-text">
                </div>
                <div class="input-form">
                    <label for="tlp">No Telepon/Handphone <em>*</em></label>
                    <input type="text" name="tlp" id="tlp" value="" class="input-text">
                </div>
                <div class="input-form">
                    <label for="alamat">Alamat <em>*</em></label>
                    <textarea name="alamat" rows="8" cols="40" class="input-text"></textarea>
                </div>
                <div class="input-form">
                    <label for="email">Email <em>*</em></label>
                    <input type="text" name="email" id="email" value="" class="input-text">
                </div>
                <div class="input-form">
                    <label for="password">Password <em>*</em></label>
                    <input type="password" name="password" id="password" value="" class="input-text">
                </div>
                <div class="input-form">
                    <label for="confirm">Confrim Password <em>*</em></label>
                    <input type="password" name="confirm" id="confirm" value="" class="input-text">
                </div>
                <div class="input-form">
                    <label for="foto">Upload Foto <em>*</em></label>
                    <input type="file" name="foto" id="foto" class="input-text">
                </div>
                <div class="input-form">
                    <div class="required">
                        * Wajib Di isi!
                    </div>
                </div>
                <div class="button-set">
                    <input type="submit" name="submit" value="Register" class="button gold">
                </div>
            </fieldset>
        </div>
      </div>
    </form>

    </div>
  </div>
  <div class="cleaner">&nbsp;</div>
</div>
